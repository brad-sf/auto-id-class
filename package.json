{
  "name": "auto-id-class",
  "version": "0.7.1",
  "description": "HTML class='' and id='' shortcuts via . and # emulating Sublime Text's auto_id_class feature.",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "repository": "https://github.com/bradleyflood/auto-id-class",
  "main": "./lib/auto-id-class.js",
  "scripts": {
    "precommit": "npm run format && npm test",
    "format": "prettier '*.{js,ts,css,less,scss,vue,json,gql,md}' --write && ./node_modules/.bin/eslint src/*.js test/*.js --fix",
    "test": "./node_modules/.bin/eslint lib/*.js"
  },
  "keywords": [
    "auto_id_class",
    "auto_class_id",
    "sublime auto id class",
    "html class shortcut",
    "html id shortcut"
  ],
  "dependencies": {},
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jasmine": "^2.9.3",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "^0.14.3",
    "prettier": "1.12.1"
  }
}
